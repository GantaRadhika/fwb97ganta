extends layout

block content
  h1= title
  - var image_names = ["image1.jpg","image2.jpg","image3.jpg","image4.jpg","image5.jpg"] 
  - let k = 3
  -while(k--)
     
     -let value=Math.floor(Math.random()*5);
     img(src=`/images/${image_names[value]}` alt=`car images ${value}` height="300px" width="300px")